		graphic.hpp \
		glutils.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o drawing.o drawing.cpp

fulfill.o: fulfill.cpp fulfill.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o fulfill.o fulfill.cpp

glstyle.o: glstyle.cpp glutils.hpp \
		glstyle.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o glstyle.o glstyle.cpp

glutils.o: glutils.cpp glutils.hpp \
		graphic.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o glutils.o glutils.cpp

graphic.o: graphic.cpp ribbon.hpp \
		graphic.hpp \
		glutils.hpp \
		glstyle.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o graphic.o graphic.cpp

mouse.o: mouse.cpp ortho.hpp \
		mouse.hpp \
		glutils.hpp \
		drawing.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mouse.o mouse.cpp

ortho.o: ortho.cpp ortho.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ortho.o ortho.cpp

ribbon.o: ribbon.cpp glutils.hpp
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ribbon.o ribbon.cpp

