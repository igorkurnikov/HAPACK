####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: Makefile $(TARGET) 

staticlib: $(TARGET)

$(TARGET):  $(OBJECTS) $(OBJCOMP) 
	-$(DEL_FILE) $(TARGET)
	$(AR) $(TARGET) $(OBJECTS)


dist: 
	@$(CHK_DIR_EXISTS) .tmp/mortgl1.0.0 || $(MKDIR) .tmp/mortgl1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/mortgl1.0.0/ && $(COPY_FILE) --parents drawing.hpp fulfill.hpp glstyle.hpp glutils.hpp graphic.hpp mouse.hpp ortho.hpp ribbon.hpp .tmp/mortgl1.0.0/ && $(COPY_FILE) --parents drawing.cpp fulfill.cpp glstyle.cpp glutils.cpp graphic.cpp mouse.cpp ortho.cpp ribbon.cpp .tmp/mortgl1.0.0/ && (cd `dirname .tmp/mortgl1.0.0` && $(TAR) mortgl1.0.0.tar mortgl1.0.0 && $(COMPRESS) mortgl1.0.0.tar) && $(MOVE) `dirname .tmp/mortgl1.0.0`/mortgl1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/mortgl1.0.0


clean: 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile

####### Compile

